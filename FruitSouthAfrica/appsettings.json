{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=FruitSouthAfrica;Trusted_Connection=True;TrustServerCertificate=true"
  },
  "Identity": {
    "User": {
      "RequireUniqueEmail": true,
      "AllowedUserNameCharacters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-._@+"
    },
    "Password": {
      "RequireDigit": true,
      "RequiredLength": 8,
      "RequireLowercase": true,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": true
    }
  }
}
